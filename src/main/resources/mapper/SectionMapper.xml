<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.framework.mapper.SectionMapper">
    <select id="textSection" resultType="map">
        SELECT * FROM section where course_id=#{course_id} and status="1"
    </select>
    <select id="textSection1" resultType="map">
        SELECT * FROM section where uuid=#{uuid}
    </select>
</mapper>